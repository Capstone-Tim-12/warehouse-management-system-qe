{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "$ref": "#/definitions/Welcome",
  "definitions": {
    "Welcome": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "status": {
          "type": "boolean"
        },
        "message": {
          "type": "string"
        },
        "data": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Datum"
          }
        },
        "pagination": {
          "$ref": "#/definitions/Pagination"
        }
      },
      "required": [
        "data",
        "message",
        "pagination",
        "status"
      ],
      "title": "Welcome"
    },
    "Datum": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "transactionId": {
          "type": "string"
        },
        "userId": {
          "type": "integer"
        },
        "username": {
          "$ref": "#/definitions/Username"
        },
        "regencyId": {
          "type": "string",
          "format": "integer"
        },
        "regencyName": {
          "$ref": "#/definitions/RegencyName"
        },
        "provinceId": {
          "type": "string",
          "format": "integer"
        },
        "provinceName": {
          "$ref": "#/definitions/ProvinceName"
        },
        "warehouseName": {
          "type": "string"
        },
        "warehouseId": {
          "type": "integer"
        },
        "duration": {
          "type": "integer"
        },
        "paymentScheme": {
          "$ref": "#/definitions/PaymentScheme"
        },
        "status": {
          "$ref": "#/definitions/Status"
        }
      },
      "required": [
        "duration",
        "paymentScheme",
        "provinceId",
        "provinceName",
        "regencyId",
        "regencyName",
        "status",
        "transactionId",
        "userId",
        "username",
        "warehouseId",
        "warehouseName"
      ],
      "title": "Datum"
    },
    "Pagination": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "page": {
          "type": "integer"
        },
        "limit": {
          "type": "integer"
        },
        "totalData": {
          "type": "integer"
        },
        "totalPage": {
          "type": "integer"
        }
      },
      "required": [
        "limit",
        "page",
        "totalData",
        "totalPage"
      ],
      "title": "Pagination"
    },
    "PaymentScheme": {
      "type": "string",
      "enum": [
        "bulanan",
        "mingguan"
      ],
      "title": "PaymentScheme"
    },
    "ProvinceName": {
      "type": "string",
      "enum": [
        "DI YOGYAKARTA",
        "JAWA BARAT",
        "ACEH"
      ],
      "title": "ProvinceName"
    },
    "RegencyName": {
      "type": "string",
      "enum": [
        "KABUPATEN SLEMAN",
        "KABUPATEN MAJALENGKA",
        "KABUPATEN ACEH SINGKIL"
      ],
      "title": "RegencyName"
    },
    "Status": {
      "type": "string",
      "enum": [
        "disetujui",
        "butuh persetujuan",
        "ditolak"
      ],
      "title": "Status"
    },
    "Username": {
      "type": "string",
      "enum": [
        "Mia Aprilia Satya",
        "Diana Pacar Wonbin"
      ],
      "title": "Username"
    }
  }
}
